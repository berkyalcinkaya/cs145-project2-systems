        "import hashlib\n",
        "\n",
        "def HASHVALUE(value, B):\n",
        "    if isinstance(value, int):\n",
        "        return hash(value) % B\n",
        "    sha256 = hashlib.sha256()\n",
        "    sha256.update(str(value).encode(\"utf-8\"))\n",
        "    return int(sha256.hexdigest(), 16) % B"
      ]
    },
